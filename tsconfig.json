{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*"]
    },
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "ES6",
    "jsx": "preserve"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    "ios",
    "android"
  ]
}